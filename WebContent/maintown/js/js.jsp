<script>
	function show() {
		document.getElementById("hide-search").style.display = 'block';
		document.getElementById("hide-on1").style.display = 'none';
		document.getElementById("hide-on2").style.display = 'none';
		document.getElementById("hide-on3").style.display = 'none';
		document.getElementById("hide-on4").style.display = 'none';
	}
	function hide() {
		document.getElementById("hide-search").style.display = 'none';
		document.getElementById("hide-on1").style.display = 'block';
		document.getElementById("hide-on2").style.display = 'block';
		document.getElementById("hide-on3").style.display = 'block';
		document.getElementById("hide-on4").style.display = 'none';
	}
	
</script>